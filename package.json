{
  "name": "@nextcss/material-colors",
  "version": "0.1.1-beta",
  "description": "material-colors module for Next.css.",
  "license": "MIT",
  "main": "dist/material-colors.css",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "nextcss"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Zsolt Tovis",
    "email": "tovis.zsolt@gmail.com",
    "url": "https://toviszsolt.github.io/"
  },
  "funding": "https://paypal.me/toviszsolt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextcss/material-colors.git"
  },
  "bugs": {
    "url": "https://github.com/nextcss/material-colors/issues"
  },
  "homepage": "https://next-css.com",
  "scripts": {
    "build": "npm run build:dist && npm run build:test",
    "watch": "node-sass --watch --output-style compact src/material-colors.scss dist/material-colors.css",
    "build:dist": "node-sass --output-style compressed src/material-colors.scss dist/material-colors.css",
    "build:test": "node-sass --output-style compact src/material-colors.scss test/material-colors.css"
  },
  "devDependencies": {
    "node-sass": "^6.0.1"
  }
}
